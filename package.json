{
  "name": "portfolio-jp",
  "private": true,
  "version": "1.0.0",
  "description": "Professional Japanese portfolio website showcasing web development, mobile apps, AI/ML projects, and technical expertise",
  "type": "module",
  "author": {
    "name": "Keishin",
    "email": "contact@keishin.dev",
    "url": "https://keishin.dev"
  },
  "keywords": [
    "portfolio",
    "react",
    "typescript",
    "vite",
    "tailwindcss",
    "three.js",
    "web-development",
    "mobile-development",
    "ai-ml",
    "japanese"
  ],
  "homepage": "https://keishin.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/keishin/portfolio-jp.git"
  },
  "bugs": {
    "url": "https://github.com/keishin/portfolio-jp/issues"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview --host",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "clean": "rm -rf dist node_modules/.vite",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "deploy": "npm run build && npm run preview"
  },
  "dependencies": {
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.3.0",
    "@supabase/supabase-js": "^2.57.4",
    "dompurify": "^3.2.7",
    "framer-motion": "^11.11.17",
    "lucide-react": "^0.344.0",
    "marked": "^16.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intersection-observer": "^9.13.1",
    "react-router-dom": "^6.28.0",
    "three": "^0.180.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/dompurify": "^3.0.5",
    "@types/marked": "^6.0.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0.180.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.1.7",
    "vite-plugin-pwa": "^0.20.5",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "packageManager": "npm@10.0.0"
}